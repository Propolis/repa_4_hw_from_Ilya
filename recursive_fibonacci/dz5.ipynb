    "def fibonacci(n):\n",
    "\n",
    "    if n <= 1:\n",
    "        return n\n",
    "    return fibonacci(n - 1) + fibonacci(n - 2)\n",
    "\n",
    "n_to_test = 26\n",
    "result = fibonacci(n_to_test)\n",
    "print(f\"{n_to_test}-е число Фибоначчи: {result}\")\n",
    "\n",
    "print(\"\\nПримеры:\")\n",
    "for i in range(n_to_test + 1):\n",
    "    print(f\"F({i}) = {fibonacci(i)}\")"
   ]
